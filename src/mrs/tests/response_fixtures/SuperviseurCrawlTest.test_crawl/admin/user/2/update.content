            <option disabled="" value="">Choisissez votre option</option>
            <option selected="selected" value="1">aaaaaaa</option>
        </select>
        
    </div>
</div>

 


<div class="row">
    <div class="select-field col s12 required multiselect" id="id_groups_container">
        <label for="id_groups">Groupes</label>
        <select id="id_groups" multiple="multiple" name="groups">
            <option disabled="" value="">Choisissez votre option</option>
            <option selected="selected" value="2">UPN</option>
            <option value="3">Stat</option>
            <option value="4">Support</option>
        </select>
        
    </div>
</div>


          </div>
          <div class="modal-footer">
            
    <button class="btn btn-primary" type="submit">Modifier</button>
    

          </div>
        
  </form>


      </div>