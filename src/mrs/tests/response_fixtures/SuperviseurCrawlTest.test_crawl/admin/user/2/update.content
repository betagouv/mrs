<div class="" id="modal-body-ajax">
        
<form action="/admin/user/2/update" class="form-invalid" data-action="submit-&gt;form#submit" data-controller="form" enctype="multipart/form-data" id="form-object-user" method="post">

          <div class="modal-content">
          
            
            <h4 class="text-default" id="modal-title-ajax">Utilisateur "a": modifier</h4>
            
          
          
  

  

  
<div class="row">
    <div class="input-field col s12 required" id="id_number_container">
        <input id="id_number" maxlength="30" name="number" type="text"/>
        <label for="id_number">Numéro d'agent</label>
        
    </div>
</div>
 

<div class="row">
    <div class="input-field col s12 required" id="id_first_name_container">
        <input id="id_first_name" maxlength="30" name="first_name" type="text" value=""/>
        <label for="id_first_name">Prénom</label>
        
    </div>
</div>
 

<div class="row">
    <div class="input-field col s12 required" id="id_last_name_container">
        <input id="id_last_name" maxlength="150" name="last_name" type="text" value=""/>
        <label for="id_last_name">Nom</label>
        
    </div>
</div>
 

<div class="row">
    <div class="input-field col s12 required" id="id_email_container">
        <input id="id_email" maxlength="254" name="email" type="email" value="a@aa.aa"/>
        <label class="active" for="id_email">Adresse électronique</label>
        
    </div>
</div>
 


<div class="row">
    <div class="select-field col s12 required multiselect" id="id_caisses_container">
        <label for="id_caisses">Caisses</label>
